{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": [1, "always"],
        "@typescript-eslint/semi": "off",
        "no-unexpected-multiline": "error"
    }
}
